{
  "name": "Like4Like",
  "description": "Like Post Instagram",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://*.cdninstagram.com/*",
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}